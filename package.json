{
  "name": "ts-logging",
  "version": "0.0.1",
  "description": "A serilog-like logging framework for javascript / typescript projects on node or in browser",
  "main": "dist/ts-logging.js",
  "scripts": {
    "clean": "rimraf dist tests coverage",
    "build": "tsc",
    "build-tests": "tsc -p ./testconfigs/tsconfig.tests.json",
    "build-tests-watch": "tsc -p ./testconfigs/tsconfig.tests.json -w",
    "rebuild": "rimraf dist && npm run build",
    "rebuild-tests": "rimraf tests coverage && npm run build-tests",
    "release": "standard-version",
    "test": "npm run test-mocha  && npm run test-karma",
    "test-rebuild": "npm run rebuild-tests && npm run test",
    "test-build": "npm run build-tests && npm run test",
    "test-watch": "concurrently \"npm run build-tests-watch\" \"npm run test-mocha-watch\" \"npm run test-karma-watch\"",
    "test-mocha": "mocha --opts testconfigs/mocha.opts",
    "test-mocha-watch": "mocha --opts testconfigs/mocha.opts -w",
    "test-karma": "karma start testconfigs/karma.conf.js --single-run",
    "test-karma-watch": "karma start testconfigs/karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gingters/ts-logging"
  },
  "engines": {
    "node": ">=4.2.0",
    "npm": ">=3.0 <4.0"
  },
  "keywords": [
    "js",
    "ts",
    "typescript",
    "logging",
    "browser",
    "node",
    "serilog",
    "structured"
  ],
  "files": [
    "dist/"
  ],
  "author": "Sebastian P.R. Gingter <sebastian@gingter.org>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/mocha": "^2.2.40",
    "browserify": "^14.1.0",
    "chai": "^3.5.0",
    "concurrently": "^3.4.0",
    "karma": "^1.5.0",
    "karma-browserify": "^5.1.1",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "mocha": "^3.2.0",
    "phantomjs": "^2.1.7",
    "rimraf": "^2.6.1",
    "standard-version": "^4.0.0",
    "typescript": "~2.1.6",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "moment": "^2.18.1",
    "rxjs": "^5.2.0"
  }
}
